<%= simple_form_for(@book) do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.input :author, class: 'form-control' %>
    <%= f.full_error :token %>
  </div>

  <div class="form-group">
    <%= f.label :PublishedDate %>
    <%= f.input :date, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.input :identifier, class: 'form-control' %>
  </div>

   <div class="form-group">
    <%= f.input :PublisherName, class: 'form-control' %>
  </div>

  <%= f.submit class: 'btn btn-primary' %>
<% end %>
